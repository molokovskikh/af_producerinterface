@model String

    <div class="form-group">
        <label>Password<strong class="required">*</strong></label>
        @Html.TextBoxFor(model => model, null, new {@type="password", @Name = "password", @class = "form-control col-sm-12 col-xs-12 col-md-12 col-lg-12" })
        @Html.ValidationMessageFor(model => model)
    </div>
