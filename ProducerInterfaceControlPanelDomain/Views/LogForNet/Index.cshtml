@model IEnumerable<ProducerInterfaceCommon.ContextModels.LogForNet>

@{
    ViewBag.Title = "Логи ошибок";
    Layout = "~/Views/Shared/_MasterPage.cshtml";
}

<h2>Лог ошибок</h2>
<hr />

@Html.Partial("Partial/PagerPArtial")

<table class="table table-bordered table-striped table-width-100prc" id="ErrorLogTable">
    <tr>
        <th>Номер</th>
        <th>Тип</th>
        <th>Узел</th>   
        <th>Сообщение</th>
        <th>Дата</th>   
    </tr>

    @foreach (var ItemLog in Model)
    {
        <tr>
            <td>@ItemLog.Id</td>
            <td>@ItemLog.Level</td>
            <td>@ItemLog.Host</td>
            <td>@ItemLog.Message</td>
            <td>@ItemLog.Date.ToString("yyyy.MM.dd")</td>
        
        </tr>
    }

</table>