@model IEnumerable<string>

@{
    ViewBag.Title = "Домащняя страница";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
   <h2>Стартовая страница Администрирования Интерфейса Производителя</h2>
</div>

  <div class="jumbotron">
    <h5>Сдесь могла бы быть ваша реклама</h5>

      <div class="row">@User.Identity.Name</div>

      @foreach(var XXX in ViewBag.ListGroup)
      {
      <div class="row">
            @Html.DisplayFor(itemModel => XXX)
      </div>      
      }

      <a class="form-control">
          @ViewBag.x
      </a>

     

  </div>

