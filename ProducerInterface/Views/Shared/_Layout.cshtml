		<!-- Техническая информация -->
	<div class="JavascriptParams" style="display: none">
	    @{
	        var dic = ViewBag.JavascriptParams as Dictionary<string, string>;
	        if (dic != null && dic.Count != 0)
	        {
	            foreach (var key in dic.Keys)
	            {
	                <div id="@key">@dic[key]</div>
	            }
	        }
	    }
