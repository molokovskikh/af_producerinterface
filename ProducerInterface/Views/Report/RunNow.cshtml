@model Quartz.Job.Models.SetDateInterval

@{
    ViewBag.Title = "Добавить расписание";
}
<link href="~/Content/css/bootstrap-datepicker3.css" rel="stylesheet" />
<section id="main">
    <div class="container">
        <div class="row">
            <div class="content blog col-sm-9 col-md-9">
                <div class="panel-heading">
                    <div class="panel-title">
                        <h1>Единоразовый запуск отчета</h1>
                    </div>
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="content blog col-sm-9 col-md-9">
                            <div class="col-md-12">
                                @using (Html.BeginForm(new { @class = "form form-horizontal" }))
                                {
                                  
                                    @Html.EditorFor(m => m.DateFrom)
                                
                                    @Html.EditorFor(m => m.DateToUi)
                               
                                    @Html.EditorFor(m => m.JobGroup)
                                 
                                    @Html.EditorFor(m => m.JobName)
                                                                 
                                    <div class="form-group">
                                        <div class="col-sm-3">
                                           <input type="submit" class="btn btn-success" value="Запустить"/>
                                        </div>
                                    </div>                                    
                                }

                                <div class="form-group">
                                    <div class="col-sm-3">
                                        @Html.ActionLink("Назад", "JobList", new { }, new { @class = "btn btn-success" })
                                    </div>
                                </div>     


                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</section>
        @section JavaScript {
            <script src="~/Content/js/bootstrap-datepicker.js"></script>
            <script src="~/Content/js/bootstrap-datepicker.ru.min.js"></script>
            <script src="~/Content/js/initDatePicker.js"></script>
        }
