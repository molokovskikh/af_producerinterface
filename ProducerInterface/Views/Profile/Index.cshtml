@{
    var currentUser = ViewBag.CurrentUser as EntityContext.ContextModels.ProducerUser;
    ViewBag.Title = "Кабинет производителя";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="jumbotron">
    <h3>Идинтификационный номер производителя: @currentUser.ProducerId.</h3>

    <hr/>

    <p class="lead">Вы вошли как @currentUser.Name.</p>
    <h3>Здесь могла бы быть ваша реклама</h3>
</div>



